{% extends 'base.html.twig' %}

{% block title %}Projets{% endblock %}

{% block body %}
    <h1>Projets</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ projet.id }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ projet.titre }}</td>
            </tr>
            <tr>
                <th>DateDebut</th>
                <td>{{ projet.dateDebut ? projet.dateDebut|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Contenu</th>
                <td>{{ projet.contenu }}</td>
            </tr>
            <tr>
                <th>Taches</th>
                <td>{{ taches. }}</td>
            </tr>
            <tr>
                <th>DateFin</th>
                <td>{{ projet.dateFin ? projet.dateFin|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_projets_index') }}">back to list</a>

    <a href="{{ path('app_projets_edit', {'id': projet.id}) }}">edit</a>

    {{ include('projets/deleteProjets.html.twig') }}
{% endblock %}
